#!/usr/bin/env python2

from time import sleep

from gevent import monkey; monkey.patch_all()

from bobby import app, sio
# from bobby.routes import connection


if __name__ == '__main__':
    try:
        sio.run(app)
        app.command_queue.join()
    except KeyboardInterrupt:
        print 'bobby out'
        pass
